# Database
# BEGIN DATABASE CONFIGURATION

# Tên cơ sở dữ liệu cần kết nối
DB_NAME=base
# Tên người dùng để đăng nhập vào cơ sở dữ liệu
DB_USER=sa
# Mật khẩu tương ứng với tài khoản DB_USER
DB_PASSWORD=svcntt
# Địa chỉ máy chủ cơ sở dữ liệu (localhost nếu chạy trên cùng máy)
DB_HOST=localhost
# Cổng kết nối đến cơ sở dữ liệu (5432 cho PostgreSQL, 1433 cho SQL Server)
DB_PORT=1433
# Có sử dụng xác thực Windows hay không (false = dùng tài khoản SQL)
DB_TRUSTED_CONNECTION=false
# Thời gian chờ tối đa khi thiết lập kết nối (tính bằng giây)
DB_TIMEOUT=30
# Số lượng kết nối tối đa trong connection pool
DB_MAX_POOL_SIZE=128
# Số lượng kết nối tối thiểu trong connection pool
DB_MIN_POOL_SIZE=8
# Thời gian chờ tối đa cho một câu lệnh SQL (tính bằng giây)
DB_COMMAND_TIMEOUT=60
# Thời gian giữ kết nối nhàn rỗi trước khi đóng (tính bằng giây)
DB_IDLE_TIMEOUT=300
# Thời gian sống tối đa của một kết nối (0 = không giới hạn)
DB_CONNECTION_LIFETIME=0
# Có bật connection pooling hay không (nên bật để tối ưu hiệu suất)
DB_POOLING=true
# Số lượng tối đa 1 batch trước khi flush xuống DB.
DB_BATCH_SIZE=100
# Thời gian flush định kỳ (ms) để tránh giữ job quá lâu.
DB_FLUSH_INTERVAL=5000

# END DATABASE CONFIGURATION



